{
  "name": "vite-app",
  "version": "0.0.0",
  "type": "module",
  "description": "City of Vernonia web app template.",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "dev": "vite",
    "lint": "eslint",
    "postinstall": "node ./scripts/postinstall.js",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "serve": "npm run build && vite preview",
    "test": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfVernonia/vite-app.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/luxon": "^3.4.2",
    "@types/papaparse": "^5.3.15",
    "ansi-regex": ">=6.1.0",
    "chalk": "^5.3.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "5.4.11"
  },
  "dependencies": {
    "@arcgis/core": "^4.31.6",
    "@esri/arcgis-rest-feature-service": "^4.0.6",
    "@esri/arcgis-rest-request": "^4.2.3",
    "@esri/calcite-components": "^2.13.2",
    "fast-xml-parser": "^4.5.0",
    "luxon": "^3.5.0",
    "papaparse": "^5.4.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 2,
    "endOfLine": "auto"
  }
}
